  "scripts": {
    "postinstall": "bower install",
    "build": "gulp build",
    "createDb": "mongo db/createDb.js",
    "importSample": "db/importCategories.sh; db/importCategoryMappings.sh; db/importExpenses.sh",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "start-webdriver": "webdriver-manager start",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "jshintConfig": {
    "globals": {
      "angular": false,
      "define": false,
      "Papa": false,
      "require": false,
      "module": false,
      "exports": false,
      "process": false,
      "inject": false
    },
    "maxparams": 30,
    "maxdepth": 6,
    "maxstatements": 200,
    "maxcomplexity": 15,
    "maxlen": 130,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "indent": 4,
    "laxbreak": true,
    "jasmine": true,
    "browser": true,
    "jquery": true,
    "devel": true
